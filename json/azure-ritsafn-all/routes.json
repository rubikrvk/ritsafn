{
  "routes": [
    {
      "route": "/*/",
      "rewrite": "/*/index.html"
    },
    {
      "route": "/*/*/",
      "rewrite": "/*/*/index.html"
    },
    {
      "route": "/*/*/*/",
      "rewrite": "/*/*/*/index.html"
    },
    {
      "route": "/*/*/*/*/",
      "rewrite": "/*/*/*/*/index.html"
    }
    // Add more rules if you have deeper nested folders
  ]
}